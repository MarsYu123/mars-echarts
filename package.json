{
  "name": "mars-echarts",
  "description": "用于基金产品的echarts组件包-vue3",
  "author": "Marsyu <zhengyu6211@outlook.com>",
  "version": "1.6.1",
  "main": "dist/mars-echarts.umd.js",
  "module": "dist/mars-echarts.es.js",
  "typings": "dist/packages/index.d.ts",
  "exports": {
    "./dist/style.css": "./dist/style.css",
    ".": {
      "import": "./dist/mars-echarts.es.js",
      "require": "./dist/mars-echarts.umd.js"
    }
  },
  "repository": "https://github.com/MarsYu123/mars-echarts.git",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "keywords": [
    "Vue3",
    "echarts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build-Ts": "node src/build/build.js",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "echarts": "^5.4.1",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.56.1",
    "ts-morph": "^17.0.1",
    "typescript": "~4.7.4",
    "vite": "^3.2.4",
    "vite-plugin-dts": "^2.0.2",
    "vue-tsc": "^1.0.9"
  }
}
